{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true
  },
  "key": "ctrl+shift+i",
  "command": "editor.action.sourceAction",
  "args": {
      "kind": "source.addMissingImports",
      "apply": "first"
  },
  "exclude": [
    "uploads"
  ],
  "editor.codeActionsOnSave": [
    "source.addMissingImports",
    "uploads", "root.ts"
  ]

  
}
